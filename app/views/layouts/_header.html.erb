<div class="wrap">
  <div class="header">
    <div class="search-bar">
      <%= form_tag(search_page_path, :method => "get",
          class: "navbar-form navbar-left") do %>
        <div class="input-group">
          <%= search_field_tag :search, params[:search], placeholder: t(".s_holder"), class: "form-control" %>
          <div class="input-group-btn">
            <%= button_tag t(".search"), :class => "btn btn-info glyphicon glyphicon-search",:name => nil %>
          </div>
        </div>
      <% end %>
    </div>
    <div class="clear"> </div>
    <div class="header-top-nav">
      <ul>
        <li><%= link_to t("base.signup"), "#" %></li>
        <li><%= link_to t("base.login"), "#" %></li>
        <li><%= link_to t("base.account"), "#" %></li>
        <li><%= link_to t("base.cart"), "#" %></li>
      </ul>
    </div>
    <div class="clear"> </div>
  </div>
</div>
<div class="clear"></div>
<div class="top-header">
  <div class="wrap">
    <div class="logo">
    <%= link_to image_tag("customer/logo.png", alt: t(".logo")),root_path %>
    </div>
    <div class="top-nav">
      <ul>
        <li><%= link_to t("base.home"), root_path %></li>
        <li><%= link_to t("base.about"), about_path %></li>
        <li><%= link_to t("base.contact"), contact_path %></li>
      </ul>
    </div>
    <div class="clear"></div>
  </div>
</div>
